<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Widget.Base.Theme.DemoApp.ButtonBar.Fullscreen" parent="">
        <item name="android:background">@color/black_overlay</item>
        <item name="android:buttonBarStyle">?android:attr/buttonBarStyle</item>
    </style>

    <style name="roundedImageView" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">20dp</item>
        <item name="color">@color/white</item>
    </style>

    <style name="RatingBar" parent="Theme.AppCompat">
        <item name="colorControlNormal">@color/BottomIconLightText</item>
        <item name="colorControlActivated">@color/TopBarLight</item>
        <item name="colorControlHighlight">@color/BottomIconLightText</item>
    </style>


    <style name="WhiteHintText.Focused" parent="TextAppearance.Design.Hint">
        <item name="android:textColor">@android:color/white</item>
    </style>



</resources>